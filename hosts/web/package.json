{
  "name": "web",
  "version": "1.0.0",
  "description": "Host shell app serving the micro-frontend runtime",
  "main": "src/index.ts",
  "scripts": {
    "build": "rspack build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json",
    "serve": "serve dist -p 2006",
    "dev": "sh -c \"node scripts/server.js & rspack serve & sleep 2 && open http://localhost:3000; wait\""
  },
  "keywords": [
    "micro-frontends",
    "discovery",
    "service"
  ],
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.26.9",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "6.21.3",
    "styled-components": "^6.1.13",
    "tseep": "^1.3.1"
  },
  "devDependencies": {
    "@rspack/cli": "^0.5.8",
    "@rspack/core": "^0.5.8",
    "@rspack/plugin-html": "^0.5.8",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@module-federation/enhanced": "^0.18.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "prettier": "^3.3.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "typescript": "^5.7.3"
  }
}
